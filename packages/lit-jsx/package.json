{
	"name": "jsx-lit",
	"description": "A JSX runtime and compiler that transforms JSX into Lit templates.",
	"license": "Apache-2.0",
	"contributors": [
		"Kristoffer Roen-Lie"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RoenLie/weave.git"
	},
	"version": "1.0.4",
	"files": [
		"dist",
		"src"
	],
	"type": "module",
	"main": "./dist/react-jsx/jsx-runtime.js",
	"types": "./dist/shared/jsx-types.d.ts",
	"exports": {
		".": "./dist/utils.js",
		"./jsx-runtime": {
			"import": "./dist/shared/jsx-types.d.ts",
			"default": "./dist/react-jsx/jsx-runtime.js"
		},
		"./jsx-dev-runtime": {
			"import": "./dist/shared/jsx-types.d.ts",
			"default": "./dist/react-jsx/jsx-runtime.js"
		},
		"./vite-jsx-react": "./dist/react-jsx/vite-plugin.js",
		"./vite-jsx-preserve": "./dist/compiler/vite-plugin.js"
	},
	"sideEffects": false,
	"scripts": {
		"dev": "vite build --watch --mode=development",
		"build": "pnpm run --sequential \"/^build::.?/\"",
		"build::js": "vite build",
		"build::types": "tsc --project ./src/tsconfig.json --noEmit false"
	},
	"dependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-syntax-jsx": "^7.18.6",
		"@babel/types": "^7.27.6",
		"@types/babel__core": "^7.20.5",
		"csstype": "^3.1.3",
		"validate-html-nesting": "^1.2.1",
		"merge-anything": "^6.0.6",
		"@types/babel__traverse": "^7.20.7",
		"@babel/traverse": "^7.27.4"
	},
	"peerDependencies": {
		"lit-html": "^3.3.0"
	},
	"devDependencies": {
		"lit-html": "^3.3.0"
	}
}
