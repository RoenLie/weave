{
	"name": "@roenlie/lit-jsx",
	"description": "A JSX runtime that transforms JSX into Lit templates.",
	"license": "Apache-2.0",
	"contributors": [
		"Kristoffer Roen-Lie"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sanguinejs/sanguinejs.git"
	},
	"version": "1.0.2",
	"files": [
		"dist",
		"src"
	],
	"type": "module",
	"main": "./dist/standard/jsx-runtime.js",
	"types": "./dist/standard/jsx-types.d.ts",
	"exports": {
		".": "./dist/standard/jsx-utils.js",
		"./jsx-runtime": {
			"types": "./dist/standard/jsx-types.d.ts",
			"default": "./dist/standard/jsx-runtime.js"
		},
		"./jsx-dev-runtime": {
			"types": "./dist/standard/jsx-types.d.ts",
			"default": "./dist/standard/jsx-runtime.js"
		},
		"./vite": "./dist/compiler/index.js"
	},
	"sideEffects": false,
	"scripts": {
		"dev": "vite build --watch --mode=development",
		"build": "pnpm run --sequential \"/^build::.?/\"",
		"build::js": "vite build",
		"build::types": "tsc --project ./src/tsconfig.json --noEmit false"
	},
	"dependencies": {
		"csstype": "^3.1.3"
	},
	"peerDependencies": {
		"lit-html": "^3.3.0"
	},
	"devDependencies": {
		"lit-html": "^3.3.0"
	}
}
