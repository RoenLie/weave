{
	"extends": "@roenlie/tsconfig",
	"compilerOptions": {
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"noEmit": false,
		"emitDeclarationOnly": true,
		"incremental": false,
		"types": [
			"node"
		]
	},
	"include": [
		"**/*.ts",
		"**/*.d.ts",
	],
	"exclude": [
		"**/*.test.ts",
		"**/*.bench.ts",
	]
}