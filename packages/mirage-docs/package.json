	"files": [
		"./dist",
		"./src",
		"./LICENSE",
		"./package.json",
		"./README.md"
	],
	"type": "module",
	"main": "./dist/app/index.js",
	"types": "./dist/app/index.d.ts",
	"exports": {
		"./server": "./dist/server/index.js",
		"./app": "./dist/app/index.js",
		"./app/*": "./dist/app/*",
		"./workers/*": "./dist/workers/*",
		"./assets/*": "./src/assets/*"
	},
	"scripts": {
		"test": "vitest --config ./tests/vite.config.ts",
		"build": "pnpm run --sequential \"/^build:.+/\"",
		"build:indexes": "pkg-toolbox build-indexes",
		"build:server": "vite build --config ./vite.config.ts",
		"build:workers": "vite build --config ./vite-workers.config.ts",
		"build:types": "tsc --project ./src/app/tsconfig.json && tsc --project ./src/server/tsconfig.json"
	},
	"dependencies": {
		"@roenlie/monaco-editor-wc": "workspace:^",
		"@roenlie/lit-aegis": "workspace:^",
		"@roenlie/core": "workspace:^",
		"@custom-elements-manifest/analyzer": "^0.10.2",
		"@orama/orama": "2.0.23",
		"@orama/plugin-data-persistence": "2.0.23",
		"@orama/plugin-match-highlight": "2.0.23",
		"@orama/plugin-parsedoc": "2.0.23",
		"@sindresorhus/slugify": "^2.2.1",
		"deepmerge-ts": "^7.0.3",
		"globby": "^14.0.0",
		"highlight.js": "^11.9.0",
		"markdown-it": "^14.0.0",
		"markdown-it-anchor": "^9.0.1",
		"rollup-plugin-copy": "^3.5.0",
		"tabbable": "^6.2.0",
		"tslib": "^2.6.2"
	},
	"peerDependencies": {
		"lit": "^3.1.0",
		"vite": "^5.4.2"
	},
	"devDependencies": {
		"@types/markdown-it": "^14.1.1",
		"@types/sharedworker": "^0.0.123",
		"bootstrap-icons": "^1.11.3"
	},
	"sideEffects": []
}