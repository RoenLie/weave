{
	"name": "@roenlie/mirage",
	"description": "Mirage project",
	"version": "1.0.0",
	"author": "Kristoffer Roen-Lie",
	"keywords": [
		"web",
		"components",
		"web-components",
		"typescript",
		"lib"
	],
	"files": [
		"LICENSE",
		"/src/**/*.ts",
		"/dist/lib/*",
		"/dist/assets/*"
	],
	"type": "module",
	"main": "./dist/lib/index.js",
	"typings": "./dist/lib/index.d.ts",
	"engines": {
		"node": ">=16.13.1"
	},
	"scripts": {
		"logon": "vsts-npm-auth -config .npmrc",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview --mode preview",
		"package": "npm run build && npm run package:move",
		"package:move": "node --no-warnings --experimental-modules --loader tsm ./scripts/move-output.ts --color",
		"clean": "rimraf node_modules dist *.tsbuildinfo",
		"clean:dist": "rimraf dist *.tsbuildinfo",
		"clean:node": "rimraf node_modules",
		"clean:vite": "rimraf node_modules/.vite",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"lint:dry": "eslint . --ext .ts --fix-dry-run",
		"updatedeps": "pnpm update --latest --interactive"
	},
	"dependencies": {
		"@eyeshare/shared": "0.2.0-ci.245",
		"@eyeshare/web-components": "0.1.1-ci.415",
		"@floating-ui/dom": "^1.0.3",
		"@lit-labs/context": "^0.1.3",
		"@lit-labs/virtualizer": "^0.7.2",
		"@roenlie/mirage-utils": "link:..\\MirageUtils",
		"lit": "^2.4.0",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@roenlie/vite-dimension-config": "link:..\\ViteDimensionConfig",
		"@eyeshare/eslint-config": "0.1.1-ci.23",
		"@eyeshare/shared-nodejs": "0.1.0-ci.16",
		"@eyeshare/tsconfig": "^0.1.1-ci.6",
		"@eyeshare/vite-plugins": "0.1.0-ci.38",
		"@types/node": "^18.11.7",
		"@types/sharedworker": "^0.0.83",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"@vaadin/router": "^1.7.4",
		"bootstrap-icons": "^1.9.1",
		"eslint": "^8.26.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-lit": "^1.6.1",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"rimraf": "^3.0.2",
		"roboto-fontface-woff": "^0.8.0",
		"ts-toolbelt": "^9.6.0",
		"tsm": "^2.2.2",
		"type-fest": "^3.1.0",
		"typescript": "^4.8.4",
		"vite": "^3.2.1",
		"vite-plugin-compression": "^0.5.1"
	}
}